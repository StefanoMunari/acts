{
  "name": "interface",
  "version": "1.0.0",
  "description": "A poc implementation of Phoenix Elixir Angular Postgres and few other things.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-client-unit": "bin/run_client_unit_tests.sh",
    "test": "mix test && npm run test-client-unit",
    "clean": "rm -rf priv/static/*",
    "copy": "cp web/static/* priv/static",
    "copy-watch": "node_modules/.bin/cpx \"web/static/*\" priv/static --watch",
    "webpack": "node_modules/.bin/webpack",
    "webpack-watch": "node_modules/.bin/webpack --watch-stdin --progress --colors",
    "webpack-test": "node_modules/.bin/webpack --config webpack-test.config.js",
    "webpack-test-e2e": "node_modules/.bin/webpack --config webpack-test-e2e.config.js",
    "typings": "typings",
    "postinstall": "typings install",
    "start": "mix phoenix.server"
  },
  "author": "Adam Keating",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/router": "^4.4.4",
    "core-js": "^2.5.1",
    "es6-shim": "^0.35.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "phoenix": "1.2.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "angular-ide": "^0.9.32",
    "awesome-typescript-loader": "^3.2.3",
    "chai": "^4.1.2",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.7",
    "css-to-string-loader": "^0.1.3",
    "file-loader": "^1.1.5",
    "globby": "^6.1.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.5",
    "node-sass": "^4.5.3",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "typescript": "^2.5.3",
    "typings": "^2.1.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1"
  }
}
